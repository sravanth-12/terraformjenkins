pipeline {
      agent any
        stages{
          stage("build") {
            steps{
              echo "to build a jenkins pipeline"
            }
          }
           stage("test") {
            steps{
              echo "to test a jenkins pipeline"
            }
          }
          
           stage("deploy") {
            steps{
              echo "to deploy a jenkins pipeline"
            }
           }
          }
        }
